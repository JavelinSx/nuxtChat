{
  "name": "nuxt-app",
  "private": true,
  "type": "module",
  "scripts": {
    "client": "nuxt dev",
    "server": "cd server && npm run dev",
    "build": "nuxt build",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare && cd server && npm install"
  },
  "dependencies": {
    "nuxt": "^3.14.1592",
    "vue": "latest",
    "vue-router": "latest"
  },
  "devDependencies": {
    "concurrently": "^9.1.0"
  }
}
